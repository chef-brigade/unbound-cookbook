# Generated by Chef
#
# Main configuration file. Overrides unbound.conf default configuration

<% node['unbound']['config'].each do |clause, config| -%>
<%= clause %>:<% if config.is_a?(String) || config.is_a?(Integer) -%> <%= config.to_s %><% end -%>
<%
  if config.is_a?(Hash)
    config.sort.each do |config_key, config_value|
      if config_value.is_a?(Array)
        config_value.each do |val|
-%>
  <%= config_key %>: <%= val %>
<%
        end
      else
-%>
  <%= config_key %>: <%= config_value %>
<%
      end
    end
  end
end
-%>
